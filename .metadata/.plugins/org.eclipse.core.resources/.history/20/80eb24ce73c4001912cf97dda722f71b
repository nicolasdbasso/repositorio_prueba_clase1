#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int uno;
	int dos;
	int res;

	uno = scanf("d%");
	dos = scanf("d%");
	res = uno + dos;
	printf("El resultado es: d%",res);
	return res;
}
