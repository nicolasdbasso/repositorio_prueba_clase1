#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int uno;
	int dos;
	int res;

	printf("Ingrese el primer digito");
	uno = scanf("%d",&uno);
	printf("Ingrese el segu");
	dos = scanf("%d",&dos);
	res = uno + dos;
	printf("El resultado es: %d",res);
	return res;
}
